{
    "name": "intbizth/linemob-core-php",
    "type": "library",
    "description": "Line Bot Translation using google translation api.",
    "keywords": [
        "line",
        "bot"
    ],
    "homepage": "https://github.com/intbizth",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "liverbool",
            "homepage": "http://github.com/liverbool"
        }
    ],
    "autoload": {
        "psr-4": {
            "LineMob\\Core\\": "src/"
        },
        "classmap": [
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "LineMob\\Core\\spec\\": "spec/"
        }
    },
    "require": {
        "php": "^5.6|^7.0",
        "linecorp/line-bot-sdk": "^3.13",

        "psr/http-message": "^1.0",
        "php-http/client-implementation": "^1.0",
        "php-http/httplug": "^1.0",
        "php-http/discovery": "^1.0",
        "symfony/workflow": "4.3",
        "league/tactician-bundle": "^1.0@dev"
    },
    "require-dev": {
        "php-http/guzzle6-adapter": "^1.0",
        "symfony/var-dumper": "^3.3",
        "doctrine/instantiator": "1.0.5",
        "doctrine/orm": "^2.5",
        "doctrine/common": "^2.8",
        "phpspec/phpspec": "^6.0@dev"
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
